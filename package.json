{
  "name": "@roseys/styler",
  "version": "0.0.34",
  "description": "WIP- My css-in-js Utility functions",
  "keywords": [],
  "author": "Joshua Rosenstein",
  "license": "MIT",
  "scripts": {
    "prepack": "yarn run build:prod",
    "clean": "rimraf node_modules && rimraf dist",
    "build:prod": "SET NODE_ENV=production && rollup -c",
    "build:dev": "rollup -c",
    "watch": "rollup -c --watch",
    "size": "bundlesize",
    "test": "jest --coverage --no-cache --ci --runInBand",
    "test:watch": "jest --watch",
    "test:quick": "jest --onlyChanged --runInBand",
    "test:coverage": "jest --coverage",
    "reinstall": "yarn run clean && yarn",
    "lint": "standard src/*.js",
    "docs": "x0 docs",
    "namedExports": "node scripts/getNamedExports.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshRosenstein/styler.git"
  },
  "main": "dist/styler.common.js",
  "module": "dist/styler.es.js",
  "jsnext:main": "dist/styler.es.js",
  "browser": "dist/styler.umd.js",
  "moduleName": "roseyStyler",
  "dependencies": {
    "@roseys/futils": "^0.0.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundlesize": "^0.17.0",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs": "^0.0.1-security",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "lint-staged": "^7.2.0",
    "mkdirp": "^0.5.1",
    "node-eval": "^2.0.0",
    "path": "^0.12.7",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.1",
    "rollup": "0.59.2",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "3.0.0",
    "standard": "^11.0.1"
  },
  "bundlesize": [
    {
      "path": "dist/*.js"
    }
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env"
          ],
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-runtime"
        ]
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "test",
      "expect"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-jsx"
    ]
  }
}
